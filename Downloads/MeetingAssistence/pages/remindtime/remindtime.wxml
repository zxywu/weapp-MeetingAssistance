<switch type="switch" bindchange="change" id="fivem" checked="{{status2}}">5分钟前提醒</switch>
</view>
<view>
<switch type="switch" bindchange="change" id="tenm" checked="{{status3}}">10分钟前提醒</switch>
</view>
<view>
<switch type="switch" bindchange="change" id="twentym" checked="{{status4}}">20分钟前提醒</switch>
</view>
<view>
<switch type="switch" bindchange="change" id="thirtym" checked="{{status5}}">30分钟前提醒</switch>
</view>
<view>
<switch type="switch" bindchange="change" id="oneh" checked="{{status6}}">1小时前提醒</switch>
</view>
<view>
<switch type="switch" bindchange="change" id="twoh" checked="{{status7}}">2小时前提醒</switch>
</view>
<button bindtap="confirm" type="primary">确定</button>